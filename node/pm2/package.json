{
  "name": "pm2",
  "version": "0.0.0",
  "scripts": {
    "config:local": "cp ./env/.env.local .env",
    "config:dev": "cp ./env/.env.dev .env ",
    "config:prod": "cp ./env/.env.prod .env",
    "_run": "nodemon",
    "_start": "node dist/index.js",
    "local": "yarn config:local && yarn _run",
    "dev": "yarn config:dev && yarn _run",
    "prod": "yarn config:prod && yarn _run",
    "migrate:dev": "yarn config:dev && prisma migrate dev",
    "build:clear": "rm -rf dist",
    "build:dev": "yarn config:dev && yarn build:clear && tsc -p tsconfig.json && tsc-alias -p tsconfig.json && yarn run config:dev",
    "build:prod": "yarn config:prod && yarn build:clear &&  tsc -p tsconfig.json && yarn run config:prod",
    "pm2:dev": "yarn build:dev && pm2 flush && pm2 start --node-args='-r dotenv/config' ecosystem.config.js",
    "pm2:prod": "yarn build:prod && pm2 flush && pm2 start --node-args='-r dotenv/config' ecosystem.config.js "
  },
  "dependencies": {
    "@adminjs/express": "^5.0.0",
    "@adminjs/prisma": "^3.0.1",
    "@fuseble.inc/node": "^0.0.1",
    "@prisma/client": "^4.2.1",
    "adminjs": "^6.1.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "hangul-js": "^0.2.6",
    "helmet": "^5.1.0",
    "module-alias": "^2.2.2",
    "nanoid": "^4.0.0",
    "node-schedule": "^2.1.0",
    "path": "^0.12.7",
    "prisma": "^4.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/node-schedule": "^2.1.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
